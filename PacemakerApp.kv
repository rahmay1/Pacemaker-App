# Egram Menu contents
<EgramWindow>

    FloatLayout:
        pos_hint: {"center_x":0.5, "center_y":0.5}
        
        BoxLayout:
            pos_hint: {"center_x":0.5, "center_y":0.5}
            orientation: "vertical"
            BoxLayout:
                size_hint: [1, 0.8]
                Graph:
                    id: egramATRGraph
                    x_grid_label: True
                    y_grid_label: True
                    tick_color: [1, 0, 0, 1]
                    xlabel: "Time" 
                    ylabel: "Volts"
                    xmin: 0
                    xmax: 150
                    ymin:-5
                    ymax: 5
                    x_ticks_major:30
                    y_ticks_major:1

                Graph:
                    id: egramVENTGraph
                    x_grid_label: True
                    y_grid_label: True
                    tick_color: [1, 0, 0, 1]
                    xlabel: "Time" 
                    ylabel: "Volts"
                    xmin: 0
                    xmax: 150
                    ymin:-5
                    ymax: 5
                    x_ticks_major:30
                    y_ticks_major:1
                    
            BoxLayout:
                pos_hint: {"center_x":0.5, "center_y":0.2}
                size_hint: [1, 0.1]
                spacing: 5
                orientation: "horizontal"
                Button:
                    background_color: (0.3, 0.66, 0.45, 1)
                    background_normal: ''
                    text: "Start Pacing"
                    bold: True
                    on_release: root.start()
                Button:
                    background_color: (0.3, 0.66, 0.45, 1)
                    background_normal: ''
                    text: "Stop Pacing"
                    bold: True
                    on_release: root.stop()