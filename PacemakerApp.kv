# Login Menu contents
<LoginMenuWindow>
    id: login_win
    name: "loginMenu"
    space_x: self.size[0]/3
    canvas.before:
        Color:
            rgba: (1,1,1, 1)
        Rectangle:
            size: self.size
            pos: self.pos

    BoxLayout:
        orientation: "vertical"
        BoxLayout:
            size_hint_y: None
            height: 50
            canvas.before:
                Color:
                    rgba: (0.06, 0.50, 0.75, 1)
                Rectangle:
                    size: self.size
                    pos: self.pos
            Label:
                text: "Sign In"
                bold: True
                size_hint_x: 0.9

        BoxLayout:
            orientation: "vertical"
            padding: login_win.space_x, 10
            spacing: 20
            BoxLayout:
                orientation: "vertical"
                spacing: 15
                size_hint_y: None
                height: 110
                Label:
                    id: infoText
                    text: ''
                    markup: True
                    size_hint_y: None
                    height: 20
                TextInput:
                    id: userText
                    hint_text: "Username"
                    multiline: False
                    focus: True
                    on_text_validate: passText.focus = True
                TextInput:
                    id: passText
                    hint_text: "Password"
                    multiline: False
                    password: True
                    on_text_validate: root.check_user()
            Label:
                id: sp
                size_hint_y: None
                height: 40
            Button:
                text: "Login"
                size_hint_y: None
                height: 40
                background_color: (0.06, 0.50, 0.75, 1)
                background_normal: ''
                on_release: root.check_user()
            Button:
                text: "Don't have an account? Signup"
                size_hint_y: None
                height: 40
                background_color: (0.06, 0.50, 0.75, 1)
                background_normal: ''
                on_release: root.register_Call()
            Label:
                id: sp2

